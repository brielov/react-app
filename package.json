{
  "name": "react-app",
  "version": "2.4.2",
  "license": "MIT",
  "repository": "git@github.com:brielov/react-app.git",
  "author": "Gabriel Vaquer <brielov@icloud.com>",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress",
    "dev": "cross-env NODE_ENV=development webpack serve --progress --color",
    "postinstall": "husky install",
    "test": "jest --passWithNoTests --verbose --maxWorkers 50%"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "workbox-window": "^6.1.5"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@svgr/webpack": "^5.5.0",
    "@swc/core": "^1.2.57",
    "@swc/helpers": "^0.2.11",
    "@swc/jest": "^0.1.2",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "autoprefixer": "^10.2.5",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-polyfill-webpack-plugin": "^1.1.2",
    "postcss": "^8.2.15",
    "postcss-loader": "^5.3.0",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.3.0",
    "swc-loader": "^0.1.13",
    "tailwindcss": "^2.1.2",
    "terser-webpack-plugin": "^5.1.2",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "workbox-webpack-plugin": "^6.1.5"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all",
      "not ie > 0"
    ]
  },
  "lint-staged": {
    "src/**/*.+(css|graphql)": "prettier --write",
    "src/**/*.ts?(x)": [
      "eslint --fix --max-warnings 0",
      "jest --bail --findRelatedTests"
    ]
  }
}
